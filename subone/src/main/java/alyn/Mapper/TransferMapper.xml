<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.alyn.sql">
    <select id="transfer_getLimit" parameterType="int" resultMap="HashMap">
        select
          *
        from
          transfer
        limit #{limit}
    </select>
</mapper>